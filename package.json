{
  "name": "mrion",
  "version": "1.0.0",
  "description": "Electron-based Manga Reader app",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder",
    "rebuild": "electron-builder install-app-deps"
  },
  "build": {
    "appId": "com.mmfallacy.mrion",
    "directories": {
      "output": "dist"
    },
    "extraFiles": [
      "userdata"
    ],
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "resources/img/icon.png"
    }
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^8.2.0",
    "electron-reload": "^1.5.0",
    "electron-builder": "^22.4.1"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "cheerio": "^1.0.0-rc.3",
    "fs": "0.0.1-security",
    "jquery": "^3.4.1",
    "knex": "^0.20.13",
    "os": "^0.1.1",
    "puppeteer": "^3.0.0",
    "sqlite3": "^4.1.1"
  }
}
